{
    "NeutronNetworks.create_and_update_networks": [
        {
            "args": {
                "network_update_args": {
                    "admin_state_up": false,
                    "name": "_updated"
                 },
                "network_create_args": {}
            },
            "runner": {
                "type": "constant",
                "times": {{run_N_times}},
                "concurrency": {{number_of_concurrent_scenarios}}
            },
            "context": {
                "users": {
                    "tenants": {{number_of_tenants}},
                    "users_per_tenant": {{users_per_tenant}}
                },
                "quotas": {
                    "neutron": {{neutron_quotas_params}}
                }
            }
        }
    ]
}
