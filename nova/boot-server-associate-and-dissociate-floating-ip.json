{
  "NovaServers.boot_server_associate_and_dissociate_floating_ip": [
    {
      "runner": {
        "type": "constant",
        "concurrency": {{number_of_concurrent_scenarios}},
        "times": {{run_N_times}}
      },
      "args": {
        "flavor": {
          "name": "{{flavor_name}}"
        },
        "image": {
          "name": "{{image_name}}"
        }
      },
      "context": {
        "users": {
          "users_per_tenant": {{users_per_tenant}},
          "tenants": {{number_of_tenants}}
        },
        "network": {}
      },
      "sla": {
        "failure_rate": {
          "max": {{sla_failure_rate_max}}
        }
      }
    }
  ]
}
